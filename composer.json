{
    "name": "yuiirax/gizmo-eloquent",
    "description": "A Eloquent ORM for Gizmo Application Management Platform",
    "authors": [
        {
            "name": "Ira Chan",
            "email": "ira@yuisolution.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.4",
        "illuminate/database": "^6.4",
        "illuminate/console": "^6.4",
        "illuminate/filesystem": "^6.4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "YuiiraX\\Gizmo\\Eloquent\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "YuiiraX\\Gizmo\\Eloquent\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YuiiraX\\Gizmo\\Eloquent\\GizmoEloquentServiceProvider"
            ]
        }
    }
}
